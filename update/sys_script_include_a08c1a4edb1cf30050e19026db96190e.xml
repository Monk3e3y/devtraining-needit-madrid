        <description>Script include to load email Id of requested for.</description>
        <name>LoadEmail</name>
        <script><![CDATA[var LoadEmail = Class.create();
LoadEmail.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	loadEmail : function(){
		var record = new GlideRecord("sys_user");
		record.get(this.getParameter("sysparm_userId"));
		return record.email + "";
	},
    type: 'LoadEmail'
});]]></script>
